{
  "manifest_version": 3,
  "name": "Google Form Auto Fill",
  "version": "1.0",
  "description": "快速填寫 Google Form 常用資料",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ]
}
